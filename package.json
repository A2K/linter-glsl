{
  "name": "linter-glsl",
  "main": "./lib/linter-glsl",
  "version": "1.0.6",
  "description": "Adds linting for GLSL shaders.",
  "repository": "https://github.com/AtomLinter/linter-glsl",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^4.2.0",
    "atom-message-panel": "^1.2.4",
    "atom-package-deps": "^3.0.7",
    "fs-extra": "^0.26.3",
    "language-glsl": "^2.0.1",
    "node-whereis": "^0.0.1",
    "temp": "^0.8.3"
  },
  "packages-deps": [
    "linter"
  ],
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
